@model personapi_dotne.Models.Entities.Telefono

@{
    ViewData["Title"] = "Detalles de Teléfono";
}

<h1 class="mb-4 text-info">Detalles de Teléfono</h1>

<div class="card shadow p-4">
    <dl class="row">
        <dt class="col-sm-3">Número</dt>
        <dd class="col-sm-9">@Model.Num</dd>

        <dt class="col-sm-3">Operador</dt>
        <dd class="col-sm-9">@Model.Oper</dd>

        <dt class="col-sm-3">Dueño</dt>
        <dd class="col-sm-9">@Model.Duenio</dd>
    </dl>

    <div class="d-flex justify-content-between mt-4">
        <a asp-action="Edit" asp-route-num="@Model.Num" class="btn btn-primary">Editar</a>
        <a asp-action="Index" class="btn btn-secondary">Volver</a>
    </div>
</div>
